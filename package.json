{
   "name": "mira-video-manager",
   "version": "1.6.0",
   "description": "Video Process for mira project",
   "main": "index.js",
   "scripts": {
      "test": "ava",
      "start:jobscheduler": "cross-env START_AS=JOB_SCHEDULER ts-node src/main.ts",
      "start:jobexecutor": "cross-env START_AS=JOB_EXECUTOR ts-node src/main.ts",
      "start:jobexecutor:meta": "cross-env START_AS=JOB_EXECUTOR EXEC_MODE=META_MODE ts-node src/main.ts",
      "start:server:api": "cross-env DEBUG=true START_AS=API_SERVER ts-node src/WebServer.ts",
      "start:server:jobexecutor": "cross-env DEBUG=true START_AS=JOB_EXECUTOR ts-node src/WebServer.ts",
      "clean:dist": "rimraf dist/*",
      "build": "npm run clean:dist && tsc && cp package.json dist",
      "prod:jobscheduler": "cross-env START_AS=JOB_SCHEDULER node dist/main.js",
      "prod:jobexecutor": "cross-env START_AS=JOB_EXECUTOR  node dist/main.js",
      "prod:jobexecutor:meta": "cross-env START_AS=JOB_EXECUTOR EXEC_MODE=META_MODE node dist/main.js",
      "prod:server:api": "cross-env DEBUG=true START_AS=API_SERVER node dist/WebServer.js",
      "prod:server:jobexecutor": "cross-env DEBUG=true START_AS=JOB_EXECUTOR node dist/WebServer.js",
      "migrate:upgrade": "ts-node src/migrate.ts --upgrade",
      "migrate:generate": "ts-node src/migrate.ts --generate",
      "migrate:init": "ts-node src/migrate.ts --init",
      "migrate:sync": "ts-node src/migrate.ts --sync",
      "migrate:sync:silent": "ts-node src/migrate.ts --sync --silent",
      "e2e:jobscheduler": "cross-env START_AS=JOB_SCHEDULER JOB_DATA_PERSIST=true ts-node src/tests/starter.e2e.ts",
      "e2e:jobexecutor": "cross-env START_AS=JOB_EXECUTOR JOB_DATA_PERSIST=true ts-node src/tests/starter.e2e.ts",
      "e2e:command": "ts-node src/tests/command.e2e.ts"
   },
   "keywords": [],
   "author": "Everett Summer",
   "license": "Apache-2.0",
   "dependencies": {
      "@aws-sdk/client-s3": "^3.820.0",
      "@cloudamqp/amqp-client": "^2.1.0",
      "@irohalab/mira-shared": "^3.7.0",
      "@mikro-orm/core": "^5.5.3",
      "@mikro-orm/migrations": "^5.5.3",
      "@mikro-orm/postgresql": "^5.5.3",
      "@sentry/node": "^7.0.0",
      "amqplib": "^0.7.1",
      "axios": "^0.21.2",
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "esprima": "^4.0.1",
      "express": "^4.17.3",
      "fast-average-color-node": "^2.6.0",
      "fontkit": "^2.0.2",
      "inversify": "^6.0.1",
      "inversify-binding-decorators": "^4.0.0",
      "inversify-express-utils": "^6.4.3",
      "inversify-logger-middleware": "^3.1.0",
      "inversify-socket-utils": "^1.0.296",
      "js-yaml": "^4.1.0",
      "nanoid": "^3.3.4",
      "pg": "^8.4.0",
      "pino": "^7.6.2",
      "reflect-metadata": "^0.1.10",
      "socket.io": "^4.5.3",
      "tail": "^2.2.4"
   },
   "devDependencies": {
      "@mikro-orm/cli": "^5.5.3",
      "@types/amqplib": "^0.8.0",
      "@types/cors": "^2.8.12",
      "@types/esprima": "^4.0.3",
      "@types/express": "^4.17.13",
      "@types/fontkit": "^2.0.1",
      "@types/js-yaml": "^4.0.2",
      "@types/node": "^16.3.3",
      "@types/offscreencanvas": "^2019.7.2",
      "@types/supertest": "^2.0.11",
      "@types/tail": "^2.2.1",
      "ava": "^4.2.0",
      "cross-env": "^7.0.3",
      "detect-libc": "^2.0.2",
      "rimraf": "^3.0.2",
      "supertest": "^6.1.4",
      "ts-node": "^10.7.0",
      "tslint": "^6.1.3",
      "typescript": "^4.6.2"
   },
   "engines": {
      "node": ">=16.0.0"
   }
}
